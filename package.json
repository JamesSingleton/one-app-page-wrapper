  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JamesSingleton/my-apartment-page-wrapper/issues"
  },
  "homepage": "https://github.com/JamesSingleton/my-apartment-page-wrapper#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "iguazu": "^3.0.0",
    "iguazu-holocron": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@americanexpress/one-app-bundler": "^6.1.1",
    "babel-preset-amex": "^3.3.0",
    "eslint": "^6.8.0",
